{
  "name": "inventory-backend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "start:test": "cross-env NODE_ENV=test npm run nodemon",
    "dev": "cross-env NODE_ENV=development npm run nodemon",
    "dev:debug": "cross-env NODE_ENV=development nodemon --exec npm run babel-node --inspect src/index.js",
    "nodemon": "nodemon --exec npm run babel-node -- src/index.js",
    "babel-node": "babel-node --presets='@babel/preset-env'",
    "build": "npm run clean && npm run build:babel",
    "build:babel":"babel src --out-dir build --ignore 'src/coverage, src/**/*.test.js'",
    "clean":"rm -rf build && mkdir build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "coverage": "cross-env NODE_ENV=test jest --coverage --runInBand"
  },
  "keywords": [],
  "author": "Ken Holston",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "core-js": "^3.16.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.3",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "regenerator-runtime": "^0.13.9",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0"
  },
  "devDependencies": {
    "@babel/node": "^7.14.9",
    "babel-jest": "^27.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
